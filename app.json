{
  "expo": {
    "name": "sosh",
    "slug": "soshworld",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sosh",
    "icon": "./assets/logo.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.342162757131-c1p9md1c4eckqg10u0e1c7dfsdbi9qbf"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.soshworld",
      "versionCode": 1
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    }
  }
}
